package notepad;

public class Notepad07 {

	public static void main(String[] args) { // 논리 연산에서 모든 항이 실행되지 않는 경우 ㅡ 단락 회로 평가

		int num = 0B00000101;
		
		System.out.println(num << 2);
		System.out.println(num >> 2);
		System.out.println(num >>> 2);
		
		System.out.println(num);
		
		num <<= 2;
		System.out.println(num);
		
			
	}

}
